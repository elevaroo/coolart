<div class="d-flex flex-column justify-content-center">
  <div class="d-flex" style="margin-left: 50px; margin-top: 10px">
    <%= link_to "Add artwork", new_artist_artwork_path(current_user.account), class:"add-button" %>
  </div>
  <div class="p-2 d-flex flex-wrap align-content-start">
    <% current_user.account.artworks.each do |artwork| %>
      <%= render "artworks/card", artwork: artwork %>
    <% end %>
  </div>
</div>
